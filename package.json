{
  "name": "@42sol/vue3-ui",
  "private": false,
  "version": "0.0.alpha-1",
  "license": "MIT",
  "author": "Dmitrii Pavlov <dm.rom.pav@gmail.com>",
  "keywords": [
    "@42sol/vue3-ui",
    "vue3",
    "vue3-ui",
    "UI",
    "UX"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/42-sol/vue3-ui"
  },
  "homepage": "https://42-sol.github.io/vue3-ui/",
  "files": [
    "src",
    "dist"
  ],
  "type": "module",
  "main": "./dist/vue3-ui.umd.js",
  "module": "./dist/vue3-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-ui.es.js",
      "require": "./dist/vue3-ui.umd.js"
    },
    "./dist/vue3-router-tab.css": {
      "import": "./dist/vue3-ui.css",
      "require": "./dist/vue3-ui.css"
    }
  },
  "types": "./dist/main.d.ts",
  "scripts": {
    "lint": "eslint \"src\" --ext .js,.ts",
    "lint:fix": "yarn run lint --fix",

    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-plugin-vue": "^9.9.0",
    "path": "^0.12.7",
    "postcss": "^8.4.21",
    "pug": "^3.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "sass": "^1.59.3",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.0.2",
    "vue-tsc": "^1.0.24"
  }
}
